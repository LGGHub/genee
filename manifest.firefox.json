{
  "name": "Genee",
  "description": "Utilities to increase productivity for genetic diagnostic professionals",
  "version": "1.0",
  "manifest_version": 3,
  "author": "admin@lgghub.com",
  "action": {
    "default_popup": "ucsc.html"
  },
  "background": {
    "scripts": ["background.js"]
  },  
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "*://*.deciphergenomics.org/*"
  , "*://*.ucsc.edu/*"
  ],
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
